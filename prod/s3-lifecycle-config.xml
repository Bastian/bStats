<?xml version="1.0" ?>
<LifecycleConfiguration xmlns="http://s3.amazonaws.com/doc/2006-03-01/">
        <Rule>
                <ID>Weekly</ID>
                <Filter>
                        <Prefix>weekly/</Prefix>
                </Filter>
                <Status>Enabled</Status>
                <Expiration>
                        <Days>30</Days>
                </Expiration>
                <NoncurrentVersionExpiration>
                        <NoncurrentDays>30</NoncurrentDays>
                </NoncurrentVersionExpiration>
        </Rule>
        <Rule>
                <ID>Daily</ID>
                <Filter>
                        <Prefix>daily/</Prefix>
                </Filter>
                <Status>Enabled</Status>
                <Expiration>
                        <Days>14</Days>
                </Expiration>
                <NoncurrentVersionExpiration>
                        <NoncurrentDays>14</NoncurrentDays>
                </NoncurrentVersionExpiration>
        </Rule>
        <Rule>
                <ID>Hourly</ID>
                <Filter>
                        <Prefix>hourly/</Prefix>
                </Filter>
                <Status>Enabled</Status>
                <Expiration>
                        <Days>2</Days>
                </Expiration>
                <NoncurrentVersionExpiration>
                        <NoncurrentDays>2</NoncurrentDays>
                </NoncurrentVersionExpiration>
        </Rule>
         <Rule>
                <ID>Postgres</ID>
                <Filter>
                        <Prefix>postgres</Prefix>
                </Filter>
                <Status>Enabled</Status>
                <Transition>
                        <Days>7</Days>
                        <StorageClass>DEEP_ARCHIVE</StorageClass>
                </Transition>
                <NoncurrentVersionExpiration>
                        <NoncurrentDays>180</NoncurrentDays>
                </NoncurrentVersionExpiration>
        </Rule>
</LifecycleConfiguration>