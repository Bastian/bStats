                        <label for="chart_title_single_linechart">Chart title</label>
                    </div>
                    <!-- Chart id -->
                    <div class="input-field">
                        <input placeholder="Give the chart an undique id (e.g. 'myChart1')"
                               id="chart_id_single_linechart" class="validate"
                               pattern="^[-_a-zA-Z0-9]+(\s[-_a-zA-Z0-9]+)*$" length="50" type="text" name="chartId">
                        <label for="chart_id_single_linechart">Chart id</label>
                    </div>
                    <!-- Line name -->
                    <div class="input-field">
                        <input placeholder="The name of the line (e.g. 'Players')" id="line_name_single_linechart"
